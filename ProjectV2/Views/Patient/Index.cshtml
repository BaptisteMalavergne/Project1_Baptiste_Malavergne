@{
    ViewData["Title"] = "Patient Index";
}

<h1>Liste des patients</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
            <th>Prénom</th>
            <th>Date de naissance</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var patient in Model)
        {
                <tr>
                    <td>@patient.LastName</td>
                    <td>@patient.FirstName</td>
                    <td>@patient.DateOfBirth.ToShortDateString()</td>
                </tr>
        }
    </tbody>
</table>
